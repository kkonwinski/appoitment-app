{% extends 'base.html.twig' %}

{% block title %}CompanyAddress index{% endblock %}

{% block body %}
    <h1>CompanyAddress index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>City</th>
                <th>PostCode</th>
                <th>Country</th>
                <th>Street</th>
                <th>BuildingNumber</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for company_address in company_addresses %}
            <tr>
                <td>{{ company_address.id }}</td>
                <td>{{ company_address.city }}</td>
                <td>{{ company_address.postCode }}</td>
                <td>{{ company_address.country }}</td>
                <td>{{ company_address.street }}</td>
                <td>{{ company_address.buildingNumber }}</td>
                <td>
                    <a href="{{ path('app_company_address_show', {'id': company_address.id}) }}">show</a>
                    <a href="{{ path('app_company_address_edit', {'id': company_address.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_company_address_new') }}">Create new</a>
{% endblock %}
